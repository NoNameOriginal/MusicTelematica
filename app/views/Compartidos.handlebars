<style>
    table{
    cellspacing: 0px;
    cellpadding: 0px;
    border: 1px solid black;
    border-collapse: collapse;
    }
    tr, td, th{
        border-collapse: collapse;
        border: 1px solid black;
    }
</style>
<h1>Compartir con migo</h1>
<table>
<tr>
    <th>Nombres</th>
    <th>Apellidos</th>
    <th>Compartido con usuario #</th>
    <th>Compartir</th>
</tr>
{{#each lista}}
<tr>
    <td >{{nombres}}</td>
    <td >{{apellidos}}</td>
    <td >{{idCompartidos}}</td>
    <td><form action="/ECompartir" method="post">
        <input type="hidden" value={{id}} name="id">
        <input type="hidden" value={{idUsuario}} name="user">
        <input type="submit" value="Eliminar compartido">
        </form>
        </td>
</tr>
{{/each}}
</table>